{
    "name": "panvagenas/wp-query-builder",
    "description": "A library for building WordPress querries the easy way",
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Panagiotis Vagenas",
            "email": "pan.vagenas@gmail.com",
            "homepage": "https://gr.linkedin.com/in/panvagenas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "codacy/coverage": "dev-master",
        "satooshi/php-coveralls": ">=0.6"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [],
        "psr-4": {
            "Pan\\": "src"
        }
    },
    "archive": {
        "exclude": [
            "docs/*",
            "tests/*"
        ]
    }

}